{
    "name": "ingenius/discounts",
    "description": "Ingenius Discounts Package",
    "type": "library",
    "license": "MIT",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Ingenius Team",
            "email": "info@ingeniusclick.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ingenius/core": "*",
        "ingenius/orders": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ingenius\\Discounts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ingenius\\Discounts\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ingenius\\Discounts\\Providers\\DiscountsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
